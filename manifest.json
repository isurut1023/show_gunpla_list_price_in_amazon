{
  "manifest_version": 3,
  "name": "ガンプラ定価ゲッター",
  "description": "Amazon などのガンプラ商品ページで即座に定価が分かります。",
  "version": "0.1.0",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_title": "Click to get Bandai Price."
  },
  "action": {
    "default_popup": "html/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
	]
}
