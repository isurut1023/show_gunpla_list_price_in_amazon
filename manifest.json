{
  "name": "getBandaiPrice",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to get Bandai Price."
  },
  "permissions": [
		"activeTab",
		"scripting"
	],
	"host_permissions": [
		"https://bandai-hobby.net/",
		"https://www.amazon.co.jp/"
	],
  "background": {
    "service_worker": "background.js"
  }
}
